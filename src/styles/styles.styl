global-reset()

*
  box-sizing border-box
  &:before
  &:after
    border-box

$green       = #328619
$green_dark  = #19370F
$green_light = #D7F3CE

html
body
  font-size 100%

body
  -webkit-font-smoothing antialiased
  font       16px/1.5 "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Geneva, Verdana, sans-serif
  background url('data:image/png;base64,')

h1
  font-family 'Lobster'
  font-size   4rem
  text-align  center
  color       $green
  padding     1rem 0 2rem
  text-shadow 0 1px 1px #000

h2
  font-size   1.2rem
  font-weight bold
  color       $green_dark
  padding     1rem 0

.container
  margin    0 auto
  max-width 60rem
  padding   0 1rem 1rem
  position  relative
  overflow  auto

.form-group
  clearfix()

label
  display block
  color   #777

.form-group
  margin-bottom 1rem

.form-actions
  text-align center

  @media (min-width:878px)
    text-align center

.form-control
  border    solid 1px #c0c9d9
  padding   .75rem
  font-size 1rem
  width     100%

.tip
  display    inline-block
  font-size  10px
  font-style normal

.error
  color     red
  font-size 10px

dl
  line-height 2
  clearfix()
dt
  float left
  clear both
  color #666
dd
  float right

.panel
  background    #fff
  padding       .5rem 1.5rem 2rem
  margin-bottom 1.25rem
  border-radius 4px
  position      relative
  overflow      auto
  box-shadow    0 2px 0 rgba(0,0,0, .2)

  @media (min-width:640px)
    float left
    width 60%

  &.primary
    @media (min-width:640px)
      width       38%
      margin-left 2%

  &.with-divider
    &:before      
      @media (min-width:878px)
        position   absolute
        top        0
        bottom     0
        left       50%
        width      1px
        background #c0c9d9
        content    ''

.sidebar
  border-top solid 1px #c0c9d9

  @media (min-width:878px)
    border-top  none
    float       left
    width       46%
    margin-left 4%

.main-section
  @media (min-width:878px)
    float         left  
    width         50%
    padding-right 1.5rem

.btn
  -webkit-appearance     none
  -webkit-font-smoothing antialiased
  background             #f1f1f1
  border                 none
  border-bottom          2px solid rgba(0, 0, 0, 0.3)
  border-radius          4px
  box-sizing             border-box
  cursor                 pointer
  display                inline-block
  font-size              1rem
  font-weight            bold
  outline                none
  padding                0.75rem 1rem
  position               relative
  text-align             center
  text-decoration        none
  user-select            none
  white-space            no-wrap
  transition             border-color 0.2s ease-out

  &:hover
    border-color rgba(0, 0, 0, 0.7)

  &.btn-primary
    background  $green
    color       #fff
    text-shadow 0 0 3px #19370F

    &:hover
      color #f1f1f1

  &#btn-reset
    float      right
    margin-top 20px

/* UTILITY*/
.text-center
  text-align center

.total
  font-weight bold
  color       #000
